<!DOCTYPE html>
<html lang="en">
    <%- include("./partials/head.ejs") %>
<body>
    <%- include("./partials/header.ejs") %>

<main>
    <div>
          <% for (animal of animals) { %>
  
        <article>
            <a href="/animals/<%= animals._id %>">
              <h2>
                <%= animals.species %> - <%= animals.extinct ? "Extinct" : "Not Extinct" %>
              </h2>
            </a>
        </article>
  
          <% } %>
    </div>
</main>
  

</body>
</html>